<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>netcoreapp2.1</TargetFramework>
    <Version>0.0.1</Version>
    <Description>Abstractions of IOC container and service locator for bootstrapping applications.</Description>
    <Copyright>© 2018 Stig Schmidt Nielsson</Copyright>
    <PackageLicenseUrl>https://opensource.org/licenses/MIT</PackageLicenseUrl>
    <PackageProjectUrl>$(GithubRepoUrl)/tree/master/$(MSBuildProjectName)</PackageProjectUrl>
    <PackageIconUrl>https://www.gravatar.com/avatar/$(GravatarHash)</PackageIconUrl>
    <RepositoryUrl>$(GithubRepoUrl)</RepositoryUrl>
    <RepositoryType>GitHub</RepositoryType>
    <PackageTags>dotnetcore;c#;IOC;servicelocator</PackageTags>
    <PackageReleaseNotes>
      # 0.0.1
      Initial commit with interfaces for IIocContainer and IServiceLocator.
    </PackageReleaseNotes>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="SimpleInjector" Version="4.3.0" />
  </ItemGroup>
</Project>
